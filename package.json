{
  "publisher": "Antonio Sarcevic",
  "name": "theme-sveltelab",
  "displayName": "SvelteLab Theme",
  "version": "0.0.1",
  "packageManager": "pnpm@8.5.0",
  "description": "SvelteLab theme for VS Code",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SarcevicAntonio/vscode-theme-sveltelab"
  },
  "bugs": {
    "url": "https://github.com/SarcevicAntonio/vscode-theme-sveltelab/issues"
  },
  "keywords": [
    "theme",
    "sveltelab",
    "light",
    "dark"
  ],
  "categories": [
    "Themes"
  ],
  "icon": "icon.png",
  "files": [
    "themes"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "SvelteLab Light",
        "uiTheme": "vs",
        "path": "./themes/sveltelab-light.json"
      },
      {
        "label": "SvelteLab Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/sveltelab-dark.json"
      },
      {
        "label": "SvelteLab Black",
        "uiTheme": "vs-dark",
        "path": "./themes/sveltelab-black.json"
      },
      {
        "label": "SvelteLab Light Soft",
        "uiTheme": "vs",
        "path": "./themes/sveltelab-light-soft.json"
      },
      {
        "label": "SvelteLab Dark Soft",
        "uiTheme": "vs-dark",
        "path": "./themes/sveltelab-dark-soft.json"
      }
    ]
  },
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"esno src/index.ts\"",
    "vscode:prepublish": "npm run build",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp --all && vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@antfu/utils": "^0.7.2",
    "@types/color": "^3.0.3",
    "@types/node": "^18.16.7",
    "color": "^4.2.3",
    "eslint": "^8.40.0",
    "esno": "^0.16.3",
    "nodemon": "^2.0.22",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vsce": "^2.15.0"
  },
  "preview": true
}
